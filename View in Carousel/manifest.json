{
	"name": "View in Carousel",
	"version": "1.1",
	"description": "View photos and users on Instagram webpages in Carousel.",
	"homepage_url": "http://carousel.mobelux.com/extension",
	"update_url": "http://carousel.mobelux.com/extension/update.xml",
	"icons": 
		{ 
			"16": "Icon-16.png",
			"48": "Icon-48.png",
			"128": "Icon-128.png"
		},
	"permissions": [
		"tabs"
	],
	"background_page": "global.html",
	"content_scripts": [
		{
			"matches": ["http://instagr.am/*", "http://instagram.com/*"], 
			"css": ["style.css"],
			"js": ["jquery-1.7.1.min.js", "end.js"] 
		}
	]
}